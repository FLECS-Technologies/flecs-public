{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://raw.githubusercontent.com/FLECS-Technologies/flecs-public/main/schemas/dos.schema.json",
  "description": "",
  "type": "object",
  "properties": {
    "_schemaVersion": {
      "type": "string",
      "minLength": 1
    },
    "time": {
      "type": "string",
      "minLength": 1
    },
    "apps": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "required": [
          "name",
        ],
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "version": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  },
  "required": [
    "_schemaVersion",
    "time",
    "apps"
  ]
}
