{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/build/amd64/cargo/build/x86_64-unknown-linux-gnu",
                "${workspaceFolder}/target"
            ],
            "defines": [
                "FLECS_EXPORT=__attribute__((visibility(\"default\")))",
                "FLECS_VERSION=\"4.0.0-snowhare\"",
                "FLECS_API_VERSION=\"2.1.0\"",
                "FLECS_GIT_SHA=\"abcd1234\"",
                "FLECS_FINAL_UNLESS_TESTED=final",
                "FLECS_CONSTEXPR_UNLESS_TESTED=constexpr"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}
